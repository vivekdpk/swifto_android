    <!-- popup_take_picture -->


    <!-- Calendar -->


    <!-- Base -->

    <style name="fill">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="wide">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- Calndar -->

    <style name="calendar_base" parent="fill">
        <item name="android:background">@android:color/white</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">5dip</item>
        <item name="android:paddingRight">5dip</item>
    </style>

    <style name="calendar_header" parent="wide">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="left_arrow_pannel" parent="wide">
        <item name="android:gravity">left</item>
    </style>

    <style name="left_arrow" parent="wrap">
        <item name="android:layout_marginLeft">15dip</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:padding">10dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:src">@drawable/left_arrow</item>
    </style>

    <style name="title" parent="wrap">
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_toRightOf">@+id/previous</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:padding">5dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:textColor">#ff006f9d</item>
        <item name="android:textSize">20dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:text">Novemer 2012</item>
    </style>

    <style name="right_arrow_pannel" parent="wide">
        <item name="android:gravity">right</item>
    </style>

    <style name="right_arrow" parent="wrap">
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginRight">15dip</item>
        <item name="android:clickable">true</item>
        <item name="android:padding">10dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:src">@drawable/right_arrow</item>
    </style>

    <style name="day_pannel" parent="wide">
        <item name="android:layout_marginTop">5dip</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginBottom">5dip</item>
    </style>

    <style name="day_view" parent="fill">
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="calendar" parent="wide">
        <item name="android:numColumns">7</item>
        <item name="android:verticalSpacing">2dip</item>
        <item name="android:horizontalSpacing">2dip</item>
        <item name="android:listSelector">#00FFFFFF</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:scrollbars">none</item>
        <item name="android:background">#ffffffff</item>
        <item name="android:padding">1dip</item>
    </style>

    <!-- Calendar item -->

    <style name="base_item_container" parent="wide">
        <item name="android:background">@android:color/white</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingTop">10dip</item>
        <item name="android:paddingBottom">10dip</item>
    </style>

    <style name="date_view" parent="wrap">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">12dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:text">1</item>
        <item name="android:layout_marginLeft">5dip</item>
        <item name="android:layout_marginRight">2dip</item>
    </style>

    <style name="count_circule" parent="wrap">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:src">@drawable/calendar_circle_blue</item>
        <item name="android:visibility">visible</item>
    </style>

    <style name="count_view" parent="wrap">
        <item name="android:text">1</item>
        <item name="android:layout_marginTop">2dip</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textSize">10dip</item>
    </style>

    <!-- Quick Actions -->

    <style name="Animations" />

    <!-- PopDownMenu -->
    <style name="Animations.PopDownMenu" />

    <style name="Animations.PopDownMenu.Center">
        <item name="@android:windowEnterAnimation">@anim/grow_from_top</item>
        <item name="@android:windowExitAnimation">@anim/shrink_from_bottom</item>
    </style>

    <style name="Animations.PopDownMenu.Left">
        <item name="@android:windowEnterAnimation">@anim/grow_from_topleft_to_bottomright</item>
        <item name="@android:windowExitAnimation">@anim/shrink_from_bottomright_to_topleft</item>
    </style>

    <style name="Animations.PopDownMenu.Right">
        <item name="@android:windowEnterAnimation">@anim/grow_from_topright_to_bottomleft</item>
        <item name="@android:windowExitAnimation">@anim/shrink_from_bottomleft_to_topright</item>
    </style>

    <style name="Animations.PopDownMenu.Reflect">
        <item name="@android:windowEnterAnimation">@anim/pump_top</item>
        <item name="@android:windowExitAnimation">@anim/disappear</item>
    </style>

    <!-- PopUpMenu -->
    <style name="Animations.PopUpMenu" />

    <style name="Animations.PopUpMenu.Center">
        <item name="@android:windowEnterAnimation">@anim/grow_from_bottom</item>
        <item name="@android:windowExitAnimation">@anim/shrink_from_top</item>
    </style>

    <style name="Animations.PopUpMenu.Left">
        <item name="@android:windowEnterAnimation">@anim/grow_from_bottomleft_to_topright</item>
        <item name="@android:windowExitAnimation">@anim/shrink_from_topright_to_bottomleft</item>
    </style>

    <style name="Animations.PopUpMenu.Right">
        <item name="@android:windowEnterAnimation">@anim/grow_from_bottomright_to_topleft</item>
        <item name="@android:windowExitAnimation">@anim/shrink_from_topleft_to_bottomright</item>
    </style>

    <style name="Animations.PopUpMenu.Reflect">
        <item name="@android:windowEnterAnimation">@anim/pump_bottom</item>
        <item name="@android:windowExitAnimation">@anim/disappear</item>
    </style>

</resources>